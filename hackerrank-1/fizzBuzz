
function fizzBuzz(n) {

    let numeros = [];
    let i = 0

    while (i <= n) {
        numeros.push(i)
        i++
    }

    for (let index = 0; index < numeros.length; index++) {
        if (numeros[index] % 3 === 0 && numeros[index] % 5 === 0) {
            console.log('FizzBuzz')
        } else if (numeros[index] % 3 === 0) {
            console.log('Fizz')
        } else if (numeros[index] % 5 === 0) {
            console.log('Buzz')
        } else console.log(index)
    }

}

